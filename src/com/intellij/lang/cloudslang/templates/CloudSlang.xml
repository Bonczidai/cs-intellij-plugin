<templateSet group="CloudSlang">
  <template name="operation" value="####################################################&#10;#!!&#10;#! @description: $operation_description$&#10;#!&#10;#! @input $input_1$: $input_1$ $standard_description$&#10;#! @input $input_2$: $input_2$ $standard_description$&#10;#! @output $output$: $output$ $standard_description$&#10;#! @result $result_1$: $result_1_description$&#10;#! @result $result_2$: $result_2_description$&#10;#!!#&#10;####################################################&#10;&#10;namespace: $namespace$&#10;&#10;operation:&#10;    name: $operation_name$&#10;&#10;    inputs:&#10;      - $input_1$&#10;      - $input_2$&#10;&#10;    java_action:&#10;      gav: $group$:$artifact$:$version$&#10;      class_name: $class$&#10;      method_name: $method$&#10;&#10;    outputs:&#10;      - $output$&#10;&#10;    results:&#10;      - $result_1$: ${$success_condition$}&#10;      - $result_2$" description="CloudSlang Template" toReformat="false" toShortenFQNames="true">
    <variable name="operation_description" expression="" defaultValue="&quot;Operation Description&quot;" alwaysStopAt="false" />
    <variable name="input_1" expression="" defaultValue="&quot;input_1&quot;" alwaysStopAt="true" />
    <variable name="input_2" expression="" defaultValue="&quot;input_2&quot;" alwaysStopAt="true" />
    <variable name="output" expression="" defaultValue="&quot;output_1&quot;" alwaysStopAt="true" />
    <variable name="result_1" expression="" defaultValue="&quot;SUCCESS&quot;" alwaysStopAt="true" />
    <variable name="result_1_description" expression="" defaultValue="&quot;Operation completed successfully.&quot;" alwaysStopAt="false" />
    <variable name="result_2" expression="" defaultValue="&quot;FAILURE&quot;" alwaysStopAt="true" />
    <variable name="result_2_description" expression="" defaultValue="&quot;Failure occured during execution.&quot;" alwaysStopAt="false" />
    <variable name="namespace" expression="relativePathToProject" defaultValue="" alwaysStopAt="true" />
    <variable name="operation_name" expression="fileNameWithoutExtension()" defaultValue="" alwaysStopAt="false" />
    <variable name="group" expression="" defaultValue="&quot;group&quot;" alwaysStopAt="true" />
    <variable name="artifact" expression="" defaultValue="&quot;artifact&quot;" alwaysStopAt="true" />
    <variable name="version" expression="" defaultValue="&quot;version&quot;" alwaysStopAt="true" />
    <variable name="class" expression="capitalize(camelCase(fileNameWithoutExtension()))" defaultValue="" alwaysStopAt="true" />
    <variable name="method" expression="" defaultValue="&quot;execute&quot;" alwaysStopAt="true" />
    <variable name="standard_description" expression="" defaultValue="&quot;description&quot;" alwaysStopAt="false" />
    <variable name="success_condition" expression="" defaultValue="&quot;returnCode == '0'&quot;" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="flow" value="####################################################&#10;#!!&#10;#! @description: $flow_description$&#10;#!&#10;#! @input $input_1$: $input_1$ $standard_description$&#10;#! @input $input_2$: $input_2$ $standard_description$&#10;#! @output $output_1$: $output_1$ $standard_description$&#10;#! @result $result_1$: $result_1_description$&#10;#! @result $result_2$: $result_2_description$&#10;#!!#&#10;####################################################&#10;&#10;namespace: $namespace$&#10;&#10;flow:&#10;    name: $flow_name$&#10;&#10;    inputs:&#10;      - $input_1$: &quot;$input_1_value$&quot;&#10;      - $input_2$: &quot;$input_2_value$&quot;&#10;&#10;    workflow:&#10;      - $step_1$:&#10;          do:&#10;            $executable_name$:&#10;              - $input_1$$standar_argument_suffix$: ${$input_1$}&#10;              - $input_2$$standar_argument_suffix$: ${$input_2$}&#10;          publish:&#10;            - $step_output$&#10;          navigate:&#10;            - $step_result_1$: $result_1$&#10;            - $step_result_2$: $result_2$&#10;&#10;    outputs:&#10;      - $output_1$: ${$step_output$}&#10;&#10;    results:&#10;      - $result_1$&#10;      - $result_2$" description="CloudSlang Template" toReformat="false" toShortenFQNames="true">
    <variable name="flow_description" expression="" defaultValue="&quot;Flow description&quot;" alwaysStopAt="false" />
    <variable name="input_1" expression="" defaultValue="&quot;input_1&quot;" alwaysStopAt="true" />
    <variable name="input_2" expression="" defaultValue="&quot;input_2&quot;" alwaysStopAt="true" />
    <variable name="output_1" expression="" defaultValue="&quot;output_1&quot;" alwaysStopAt="true" />
    <variable name="result_1" expression="" defaultValue="&quot;SUCCESS&quot;" alwaysStopAt="true" />
    <variable name="result_1_description" expression="" defaultValue="&quot;Flow completed successfully.&quot;" alwaysStopAt="false" />
    <variable name="result_2" expression="" defaultValue="&quot;FAILURE&quot;" alwaysStopAt="true" />
    <variable name="result_2_description" expression="" defaultValue="&quot;Failure occured during execution.&quot;" alwaysStopAt="false" />
    <variable name="namespace" expression="relativePathToProject" defaultValue="" alwaysStopAt="false" />
    <variable name="flow_name" expression="fileNameWithoutExtension()" defaultValue="" alwaysStopAt="false" />
    <variable name="input_1_value" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="input_2_value" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="step_1" expression="" defaultValue="&quot;step_1&quot;" alwaysStopAt="true" />
    <variable name="executable_name" expression="" defaultValue="&quot;operation_name&quot;" alwaysStopAt="true" />
    <variable name="step_output" expression="" defaultValue="&quot;step_output_1&quot;" alwaysStopAt="true" />
    <variable name="step_result_1" expression="" defaultValue="&quot;SUCCESS&quot;" alwaysStopAt="true" />
    <variable name="step_result_2" expression="" defaultValue="&quot;FAILURE&quot;" alwaysStopAt="true" />
    <variable name="standar_argument_suffix" expression="" defaultValue="_arg" alwaysStopAt="false" />
    <variable name="standard_description" expression="" defaultValue="&quot;description&quot;" alwaysStopAt="false" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="input" value="- $1_input_name$:&#10;    required: $2_true_false$&#10;    default: $3_default_value$&#10;    private: $4_true_false$&#10;    sensitive: $5_true_false$" description="CloudSlang Template" toReformat="false" toShortenFQNames="true">
    <variable name="1_input_name" expression="" defaultValue="&quot;input_name&quot;" alwaysStopAt="true" />
    <variable name="2_true_false" expression="" defaultValue="&quot;true|false&quot;" alwaysStopAt="true" />
    <variable name="3_default_value" expression="" defaultValue="&quot;default_value&quot;" alwaysStopAt="true" />
    <variable name="4_true_false" expression="" defaultValue="&quot;true|false&quot;" alwaysStopAt="true" />
    <variable name="5_true_false" expression="" defaultValue="&quot;true|false&quot;" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="for" value="- $1_step_name$:&#10;    loop:&#10;      for: $2_iteration_statement$&#10;      do:&#10;        $3_executable_name$:&#10;          - $4_executable_argument$&#10;      publish:&#10;        - $5_publish_name$: $6_publish_value$&#10;      break:&#10;        - $7_result_name$&#10;    navigate:&#10;      - $8_executable_result_name$: $9_navigate_to$" description="CloudSlang Template" toReformat="false" toShortenFQNames="true">
    <variable name="1_step_name" expression="" defaultValue="&quot;step_name&quot;" alwaysStopAt="true" />
    <variable name="2_iteration_statement" expression="" defaultValue="&quot;iteration_statement&quot;" alwaysStopAt="true" />
    <variable name="3_executable_name" expression="" defaultValue="&quot;executable_name&quot;" alwaysStopAt="true" />
    <variable name="4_executable_argument" expression="" defaultValue="&quot;executable_argument&quot;" alwaysStopAt="true" />
    <variable name="5_publish_name" expression="" defaultValue="&quot;publish_name&quot;" alwaysStopAt="true" />
    <variable name="6_publish_value" expression="" defaultValue="&quot;publish_value&quot;" alwaysStopAt="true" />
    <variable name="7_result_name" expression="" defaultValue="&quot;result_name&quot;" alwaysStopAt="true" />
    <variable name="8_executable_result_name" expression="" defaultValue="&quot;executable_result_name&quot;" alwaysStopAt="true" />
    <variable name="9_navigate_to" expression="" defaultValue="&quot;navigate_to&quot;" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="java_action" value="java_action:&#10;    gav: '$1_group$:$2_artifact$:$3_version$'&#10;    class_name: $4_java_class_name$&#10;    method_name: $5_java_method_name$" description="CloudSlang Template" toReformat="false" toShortenFQNames="true">
    <variable name="1_group" expression="" defaultValue="&quot;group&quot;" alwaysStopAt="true" />
    <variable name="2_artifact" expression="" defaultValue="&quot;artifact&quot;" alwaysStopAt="true" />
    <variable name="3_version" expression="" defaultValue="&quot;version&quot;" alwaysStopAt="true" />
    <variable name="4_java_class_name" expression="" defaultValue="&quot;java_class_name&quot;" alwaysStopAt="true" />
    <variable name="5_java_method_name" expression="" defaultValue="&quot;java_method_name&quot;" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="on_failure" value="- on_failure:&#10;    - $1_step_name$:&#10;        do:&#10;          $2_executable_name$:&#10;            - $3_executable_argument$&#10;        publish:&#10;          - $4_publish_name$: $5_publish_value$" description="CloudSlang Template" toReformat="false" toShortenFQNames="true">
    <variable name="1_step_name" expression="" defaultValue="&quot;step_name&quot;" alwaysStopAt="true" />
    <variable name="2_executable_name" expression="" defaultValue="&quot;executable_name&quot;" alwaysStopAt="true" />
    <variable name="3_executable_argument" expression="" defaultValue="&quot;executable_argument&quot;" alwaysStopAt="true" />
    <variable name="4_publish_name" expression="" defaultValue="&quot;publish_name&quot;" alwaysStopAt="true" />
    <variable name="5_publish_value" expression="" defaultValue="&quot;publish_value&quot;" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="output" value="- $1_output_name$:&#10;    value: $2_output_value$&#10;    sensitive: $3_sensitive_value$" description="CloudSlang Template" toReformat="false" toShortenFQNames="true">
    <variable name="1_output_name" expression="" defaultValue="&quot;output_name&quot;" alwaysStopAt="true" />
    <variable name="2_output_value" expression="" defaultValue="&quot;output_value&quot;" alwaysStopAt="true" />
    <variable name="3_sensitive_value" expression="" defaultValue="&quot;sensitive_value&quot;" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="parallel_loop" value="- $1_step_name$:&#10;    parallel_loop:&#10;      for: $2_iteration_statement$&#10;      do:&#10;        $3_executable_name$:&#10;          - $4_executable_argument$&#10;    publish:&#10;      - $5_publish_name$: $6_publish_value$&#10;    navigate:&#10;      - $7_executable_result_name$: $8_navigate_to$" description="CloudSlang Template" toReformat="false" toShortenFQNames="true">
    <variable name="1_step_name" expression="" defaultValue="&quot;step_name&quot;" alwaysStopAt="true" />
    <variable name="2_iteration_statement" expression="" defaultValue="&quot;iteration_statement&quot;" alwaysStopAt="true" />
    <variable name="3_executable_name" expression="" defaultValue="&quot;executable_name&quot;" alwaysStopAt="true" />
    <variable name="4_executable_argument" expression="" defaultValue="&quot;executable_argument&quot;" alwaysStopAt="true" />
    <variable name="5_publish_name" expression="" defaultValue="&quot;publish_name&quot;" alwaysStopAt="true" />
    <variable name="6_publish_value" expression="" defaultValue="&quot;publish_value&quot;" alwaysStopAt="true" />
    <variable name="7_executable_result_name" expression="" defaultValue="&quot;executable_result_name&quot;" alwaysStopAt="true" />
    <variable name="8_navigate_to" expression="" defaultValue="&quot;navigate_to&quot;" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="properties" value="namespace: $1_current_namespace$&#10;&#10;properties:&#10;  - $2_property_key$: $3_property_value$" description="CloudSlang Template" toReformat="false" toShortenFQNames="true">
    <variable name="1_current_namespace" expression="" defaultValue="&quot;current.namespace&quot;" alwaysStopAt="true" />
    <variable name="2_property_key" expression="" defaultValue="&quot;property_key&quot;" alwaysStopAt="true" />
    <variable name="3_property_value" expression="" defaultValue="&quot;property_value&quot;" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="python_action" value="python_action:&#10;    dependencies:&#10;        - '$1_group1$:$2_artifact1$:$3_version1$'&#10;        - '$4_group2$:$5_artifact2$:$6_version2$'&#10;    script: $7_python_script$" description="CloudSlang Template" toReformat="false" toShortenFQNames="true">
    <variable name="1_group1" expression="" defaultValue="&quot;group1&quot;" alwaysStopAt="true" />
    <variable name="2_artifact1" expression="" defaultValue="&quot;artifact1&quot;" alwaysStopAt="true" />
    <variable name="3_version1" expression="" defaultValue="&quot;version1&quot;" alwaysStopAt="true" />
    <variable name="4_group2" expression="" defaultValue="&quot;group2&quot;" alwaysStopAt="true" />
    <variable name="5_artifact2" expression="" defaultValue="&quot;artifact2&quot;" alwaysStopAt="true" />
    <variable name="6_version2" expression="" defaultValue="&quot;version2&quot;" alwaysStopAt="true" />
    <variable name="7_python_script" expression="" defaultValue="&quot;python_script&quot;" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="step" value="- $1step_name$:&#10;    do:&#10;      $2executable_name$:&#10;        - $3executable_argument$&#10;    publish:&#10;      - $4publish_name$: $5publish_value$&#10;    navigate:&#10;      - $6executable_result_name$: $7navigate_to$" description="CloudSlang Template" toReformat="false" toShortenFQNames="true">
    <variable name="1step_name" expression="" defaultValue="&quot;step_name&quot;" alwaysStopAt="true" />
    <variable name="2executable_name" expression="" defaultValue="&quot;executable_name&quot;" alwaysStopAt="true" />
    <variable name="3executable_argument" expression="" defaultValue="&quot;executable_argument&quot;" alwaysStopAt="true" />
    <variable name="4publish_name" expression="" defaultValue="&quot;publish_name&quot;" alwaysStopAt="true" />
    <variable name="5publish_value" expression="" defaultValue="&quot;publish_value&quot;" alwaysStopAt="true" />
    <variable name="6executable_result_name" expression="" defaultValue="&quot;executable_result_name&quot;" alwaysStopAt="true" />
    <variable name="7navigate_to" expression="" defaultValue="&quot;navigate_to&quot;" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="system_property" value="- $1_property_key$:&#10;  value: $2_property_value$&#10;  sensitive: $3_sensitive_value$" description="CloudSlang Template" toReformat="false" toShortenFQNames="true">
    <variable name="1_property_key" expression="" defaultValue="&quot;property_key&quot;" alwaysStopAt="true" />
    <variable name="2_property_value" expression="" defaultValue="&quot;property_value&quot;" alwaysStopAt="true" />
    <variable name="3_sensitive_value" expression="" defaultValue="&quot;sensitive_value&quot;" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
</templateSet>