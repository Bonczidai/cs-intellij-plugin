  <!-- uncomment to enable plugin in all products
  <depends>com.intellij.modules.lang</depends>
  -->

    <extensions defaultExtensionNs="com.intellij">
        <!-- Add your extensions here -->
        <lang.parserDefinition language="CloudSlang" implementationClass="com.intellij.lang.cloudslang.plugin.CloudSlangParserDefinition"/>
        <fileTypeFactory implementation="com.intellij.lang.cloudslang.CloudSlangFileTypeFactory"/>
        <!-- Add your extensions here -->
        <completion.contributor language="CloudSlang" implementationClass="com.intellij.lang.cloudslang.parsing.CloudSlangCompletionContributor"/>
        <lang.commenter language="CloudSlang" implementationClass="com.intellij.lang.cloudslang.parsing.CloudSlangCommenter"/>
        <lang.syntaxHighlighterFactory key="CloudSlang" implementationClass="com.intellij.lang.cloudslang.parsing.CloudSlangSyntaxHighlighterFactory"/>
        <codeStyleSettingsProvider implementation="com.intellij.lang.cloudslang.plugin.CloudSlangCodeStyleSettingsProvider"/>
        <langCodeStyleSettingsProvider implementation="com.intellij.lang.cloudslang.plugin.CouldSlangLanguageCodeStyleSettingsProvider"/>


